{
    "name": "chindit/phpcs-extra",
    "description": "Extra rules for PHPCS",
    "require": {
        "php": "^8.1",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "David Lumaye",
            "email": "littletiger58@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Chindit\\": "src/Chindit/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chindit\\Tests\\": "tests"
